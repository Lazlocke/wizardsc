del chip\orig_iph\*.png
for /r %%v in (chip\orig_iph\*.iph) do tools\IPHtoPNG.exe "%%v"
mkdir chip\orig_png
move chip\orig_iph\*.png chip\orig_png\
mkdir chip\orig_png\metadata
copy chip\orig_iph\metadata chip\orig_png\metadata



del etc\orig_iph\*.png
for /r %%v in (etc\orig_iph\*.iph) do tools\IPHtoPNG.exe "%%v"
mkdir etc\orig_png
move etc\orig_iph\*.png etc\orig_png\
mkdir etc\orig_png\metadata
copy etc\orig_iph\metadata etc\orig_png\metadata
